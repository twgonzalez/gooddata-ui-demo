{
    "parser": "babel-eslint",
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier/react"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react",
        "prettier"
    ],
    "rules": {
        "prettier/prettier": "error"
    }
}
